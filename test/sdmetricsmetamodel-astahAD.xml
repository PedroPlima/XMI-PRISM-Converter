	<modelelement name="activityDiagram">
		<attribute name="name" type="data" multiplicity="one" />
		<attribute name="id" type="data" multiplicity="one" />
		<attribute name="nodes" type="ref" multiplicity="many" />
		<attribute name="edges" type="ref" multiplicity="many" />
		<attribute name="groups" type="ref" multiplicity="many" />
	</modelelement>
	
</sdmetricsmetamodel>
